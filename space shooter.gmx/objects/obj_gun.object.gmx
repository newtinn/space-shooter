    if (global.gun == 1) {
        if (global.mute == false) audio_play_sound(snd_shoot_boss2,2,false);
        bullet.direction += irandom_range(-2,2);
    }
    bullet.image_angle = bullet.direction;
    bullet.speed = 12.5;
    bullet.origSpeed = 12.5;
    
    wait = true;
    alarm[0] = 7.5;
}
